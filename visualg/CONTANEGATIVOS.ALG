Algoritmo "ContaNegativos"

Var

   N, C, TotN: Inteiro
Inicio
      C<-1
     Repita
           Escreva ("Digite um número: ")
           Leia (N)
           Se (N<0) entao
              TotN <- TotN+1
           FimSe
           C <- C+1
     Ate (C>5)
     Escreval ("Foram digitados ", TotN, "números negativos.")


Fimalgoritmo